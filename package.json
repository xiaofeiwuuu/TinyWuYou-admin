{
  "name": "miniprogram-admin",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "vue",
    "vue3",
    "admin",
    "miniprogram"
  ],
  "homepage": "https://github.com/xiaofeiwuuu",
  "bugs": "https://github.com/xiaofeiwuuu/issues",
  "license": "MIT",
  "author": {
    "name": "xiaofeiwu",
    "email": "3176430647@qq.com",
    "url": "https://github.com/xiaofeiwuuu"
  },
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 turbo build",
    "build:antd": "pnpm run build --filter=@vben/web-antd",
    "dev": "turbo-run dev",
    "dev:antd": "pnpm -F @vben/web-antd run dev",
    "clean": "node ./scripts/clean.mjs",
    "format": "vsh lint --format",
    "lint": "vsh lint",
    "postinstall": "pnpm -r run stub --if-present",
    "preinstall": "npx only-allow pnpm",
    "preview": "turbo-run preview",
    "reinstall": "pnpm clean --del-lock && pnpm install"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "catalog:",
    "@vben/commitlint-config": "workspace:*",
    "@vben/eslint-config": "workspace:*",
    "@vben/prettier-config": "workspace:*",
    "@vben/stylelint-config": "workspace:*",
    "@vben/tailwind-config": "workspace:*",
    "@vben/tsconfig": "workspace:*",
    "@vben/turbo-run": "workspace:*",
    "@vben/vite-config": "workspace:*",
    "@vben/vsh": "workspace:*",
    "@vitejs/plugin-vue": "catalog:",
    "@vitejs/plugin-vue-jsx": "catalog:",
    "autoprefixer": "catalog:",
    "cross-env": "catalog:",
    "rimraf": "catalog:",
    "tailwindcss": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vite": "catalog:",
    "vue": "catalog:",
    "vue-tsc": "catalog:"
  },
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=9.12.0"
  },
  "packageManager": "pnpm@10.14.0",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "*"
      }
    },
    "overrides": {
      "@ast-grep/napi": "catalog:",
      "@ctrl/tinycolor": "catalog:",
      "clsx": "catalog:",
      "esbuild": "0.25.3",
      "pinia": "catalog:",
      "vue": "catalog:"
    },
    "neverBuiltDependencies": [
      "canvas",
      "node-gyp"
    ]
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "jsencrypt": "^3.5.4"
  }
}
